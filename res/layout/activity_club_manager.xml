<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bb"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.myclubmy.ClubManagerActivity" >

    <Button
        android:id="@+id/buttonback3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/textView1"
        android:background="@drawable/button_back_selector" />

    <Button
        android:id="@+id/buttonclubmanager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textViewAttempt"
        android:layout_centerVertical="true"
        android:background="@drawable/button_club_manager_selector" />

    <Button
        android:id="@+id/buttoncreatclub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonclubmanager"
        android:layout_alignLeft="@+id/buttonclubmanager"
        android:layout_marginBottom="26dp"
        android:background="@drawable/button_club_activity_selector" />

    <TextView
        android:id="@+id/textViewAttempt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/buttonback3"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="软件学院学生会XX部"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/buttonhomemyplan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button_home_selector" />

    <Button
        android:id="@+id/buttonplaygame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonclubmanager"
        android:layout_below="@+id/buttonclubmanager"
        android:background="@drawable/button_game_selector" />

    <Button
        android:id="@+id/buttonresourceshare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/buttonplaygame"
        android:layout_alignRight="@+id/buttonclubmanager"
        android:layout_alignTop="@+id/buttonplaygame"
        android:layout_toRightOf="@+id/buttonplaygame"
        android:background="@drawable/button_share_selector" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/buttonback3"
        android:layout_alignBottom="@+id/buttonback3"
        android:layout_alignParentLeft="true"
        android:text="aaaaa"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#00000000" />

</RelativeLayout>
