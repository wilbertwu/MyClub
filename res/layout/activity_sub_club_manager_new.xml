<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bb"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.myclubmy.SubClubManagerNewActivity" >

    <Button
        android:id="@+id/buttonback9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/buttonclubactivity"
        android:background="@drawable/button_back_selector" />

    <Button
        android:id="@+id/buttonhome3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button_home_selector" />

    <Button
        android:id="@+id/buttonclubactivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonmembermanager"
        android:layout_below="@+id/buttonback9"
        android:background="@drawable/button_activitymanager_selector" />

    <Button
        android:id="@+id/buttonworknote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonmembermanager"
        android:layout_below="@+id/buttonclubactivity"
        android:background="@drawable/button_work_note_selector" />

    <Button
        android:id="@+id/buttonmembermanager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonmoneymanager"
        android:layout_below="@+id/buttonworknote"
        android:background="@drawable/button_member_manager_selector" />

    <Button
        android:id="@+id/buttonmoneymanager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/buttonmembermanager"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button_money_selector" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonback9"
        android:layout_below="@+id/buttonback9"
        android:text="aaaaa"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#00000000" />

</RelativeLayout>
